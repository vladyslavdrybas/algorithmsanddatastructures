{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "beberlei/DoctrineExtensions": "1.1.5",
        "ramsey/uuid": "3.8.0",
        "symfony/monolog-bundle": "3.3.1",
        "symfony/orm-pack": "1.0.5",
        "symfony/validator": "4.2.1",
        "predis/predis": "1.1.1",
        "snc/redis-bundle": "^2.1.7",
        "doctrine/doctrine-migrations-bundle": "1.3.1",
        "gedmo/doctrine-extensions": "2.4.9",
        "stof/doctrine-extensions-bundle": "1.3.0",
        "symfony/console": "4.2.1",
        "symfony/dotenv": "4.2.1",
        "symfony/flex": "1.1",
        "symfony/framework-bundle": "4.2.1",
        "symfony/yaml": "4.2.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.1",
        "symfony/var-dumper": "^4.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    }
}
